{
  "name": "matrix-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev:reg": "NODE_NO_WARNINGS=1 ts-node --esm src/register.ts",
    "dev:login": "NODE_NO_WARNINGS=1 ts-node --esm src/login.ts",
    "dev": "NODE_NO_WARNINGS=1 nodemon --watch src --exec ts-node --esm src/index.ts",
    "build": "rm -f tsconfig.tsbuildinfo && yarn tsc",
    "start": "node --enable-source-maps dist/index.js",
    "typecheck": "yarn tsc"
  },
  "type": "module",
  "dependencies": {
    "@keyv/redis": "^2.5.7",
    "@keyv/sqlite": "^3.6.5",
    "@types/needle": "^3.2.0",
    "axios": "^1.3.5",
    "chatgpt": "^5.2.2",
    "dotenv": "^16.0.3",
    "http-proxy-agent": "^5.0.0",
    "keyv": "^4.5.2",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "matrix-bot-sdk": "^0.6.4",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "typescript": "^5.0.3",
    "znv": "^0.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
